{
  "name": "bloodhound",
  "version": "0.0.1",
  "description": "bloodhound",
  "author": "Rally Software",
  "config": {
    "bloodhoundPort": "9000",
    "bloodhoundHost": "0.0.0.0"
  },
  "scripts": {},
  "jest": {
    "scriptPreprocessor": "<rootDir>/test-utils/preprocessor.js",
    "unmockedModulePathPatterns": [
      "<rootDir>/test-utils",
      "<rootDir>/utils",
      "<rootDir>/components/utils",
      "<rootDir>/components/mixins/Namespacing",
      "<rootDir>/?.*/node_modules/babel",
      "<rootDir>/?.*/node_modules/babel-runtime",
      "<rootDir>/?.*/node_modules/classnames",
      "<rootDir>/?.*/node_modules/lodash",
      "<rootDir>/?.*/node_modules/react",
      "<rootDir>/?.*/node_modules/immutable",
      "<rootDir>/?.*/node_modules/react-suitcssify",
      "<rootDir>/?.*/node_modules/react-a11y"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/test-utils/setupTestFramework.js",
    "testPathDirs": [
      "components",
      "demo",
      "test-utils/visual-diff-utils"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cthulhu-bot/bloodhound.git"
  },
  "main": "components/sombrero.js",
  "dependencies": {
    "react": "^0.14.0",
    "react-css-modules": "^3.6.1",
    "react-dom": "^0.14.0"
  },
  "devDependencies": {
    "babel-core": "^6.0.20",
    "babel-loader": "^6.0.1",
    "babel-plugin-transform-object-assign": "^6.1.18",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-react": "^6.0.15",
    "babel-preset-stage-0": "^6.0.15",
    "css-loader": "^0.15.1",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^0.9.1",
    "style-loader": "^0.13.0",
    "webpack": "^1.9.6",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.0.0"
  },
  "devEngines": {
    "node": "0.10.41",
    "npm": "2.14.13"
  }
}
