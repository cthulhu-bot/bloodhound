{
  "name": "sombrero",
  "version": "16.3.2",
  "description": "sombrero",
  "author": "Rally Software",
  "config": {
    "sombreroPort": "9000",
    "sombreroHost": "0.0.0.0"
  },
  "scripts": {
    "check-engines": "node node_modules/fbjs-scripts/node/check-dev-engines.js package.json",
    "ci-verify": "npm run clean-dist && npm test && grunt build",
    "ci-verify-and-publish": "npm run ci-verify && grunt bump-only changelog bump-commit npm-publish",
    "clean-dist": "rm -rf ./dist",
    "demo-server": "npm run clean-dist && grunt build && npm run webpack-dev-server",
    "eslint": "eslint .",
    "kill-node-inspector": "bin/killNodeInspectorProcess",
    "lint": "grunt sasslint rallylint && eslint .",
    "open-demo": "open http://localhost:${npm_package_config_sombreroPort}",
    "pretest": "npm run check-engines",
    "prestart": "npm run check-engines",
    "start": "npm run clean-dist && grunt build && npm run open-demo && npm run webpack-dev-server",
    "test": "bin/test",
    "visual-diff": "rm -rf ./screenshots && bin/checkGraphicsMagick && grunt webdriver",
    "webpack-dev-server": "webpack-dev-server --config webpack.config.demo.js"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/test-utils/preprocessor.js",
    "unmockedModulePathPatterns": [
      "<rootDir>/test-utils",
      "<rootDir>/utils",
      "<rootDir>/components/utils",
      "<rootDir>/components/mixins/Namespacing",
      "<rootDir>/?.*/node_modules/babel",
      "<rootDir>/?.*/node_modules/babel-runtime",
      "<rootDir>/?.*/node_modules/classnames",
      "<rootDir>/?.*/node_modules/lodash",
      "<rootDir>/?.*/node_modules/rally-jasmine-matchers",
      "<rootDir>/?.*/node_modules/react",
      "<rootDir>/?.*/node_modules/immutable",
      "<rootDir>/?.*/node_modules/react-suitcssify",
      "<rootDir>/?.*/node_modules/react-a11y"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/test-utils/setupTestFramework.js",
    "testPathDirs": [
      "components",
      "demo",
      "test-utils/visual-diff-utils"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/RallySoftware/sombrero.git"
  },
  "main": "components/sombrero.js",
  "dependencies": {},
  "devDependencies": {
    "artifactory-api": "^0.0.2",
    "autoprefixer-loader": "^2.0.0",
    "babel": "^5.4.7",
    "babel-core": "^5.4.7",
    "babel-eslint": "4.1.6",
    "babel-loader": "^5.1.3",
    "babel-runtime": "^5.4.7",
    "bootstrap": "3.1.1",
    "classnames": "^1.2.0",
    "consolelog": "^2.1.3",
    "css-loader": "^0.14.5",
    "del": "^1.2.1",
    "eslint": "^1.10.3",
    "eslint-config-rally": "^1.2.0",
    "eslint-plugin-jasmine": "^1.6.0",
    "eslint-plugin-rally": "^0.4.0",
    "eslint-plugin-react": "^3.11.3",
    "eslint-plugin-whitespace": "^0.1.3",
    "extract-text-webpack-plugin": "^0.3.8",
    "fbjs-scripts": "^0.5.0",
    "gm": "^1.18.0",
    "grunt": "^0.4.5",
    "grunt-bless": "BlessCSS/grunt-bless#56a062ebea",
    "grunt-bump": "0.3.1",
    "grunt-cli": "^0.1.13",
    "grunt-contrib-concat": "^0.5.0",
    "grunt-contrib-copy": "^0.7.0",
    "grunt-contrib-cssmin": "^0.12.1",
    "grunt-contrib-watch": "^0.6.1",
    "grunt-curl": "^2.1.0",
    "grunt-npm": "0.0.2",
    "grunt-rally-lint": "^0.3.0",
    "grunt-sass-lint": "0.1.0-beta.4",
    "grunt-shell": "^1.1.1",
    "grunt-wait-server": "^0.1.2",
    "grunt-webdriver": "^0.5.2",
    "grunt-webpack": "^1.0.8",
    "immutable": "^3.6.4",
    "jest-cli": "0.4.3",
    "jquery": "^2.1.3",
    "load-grunt-config": "^0.19.0",
    "lodash": "^2.4.1",
    "markdown": "^0.5.0",
    "mkdirp": "^0.5.1",
    "node-inspector": "^0.7.4",
    "node-uuid": "^1.4.3",
    "promise-loader": "^0.1.3",
    "rally-jasmine-matchers": "^2.1.0",
    "react-a11y": "^0.2.6",
    "react-dnd": "0.9.6-rally.5",
    "react-hot-loader": "^1.2.7",
    "react-redux": "^2.1.2",
    "react-router": "^0.13.2",
    "react-stats": "0.0.5",
    "react-suitcssify": "^3.0.1",
    "react-tools": "0.13.3",
    "redux": "^3.0.0",
    "redux-logger": "^1.0.8",
    "sass-loader": "^0.3.0",
    "style-loader": "^0.12.3",
    "tar.gz": "^1.0.0",
    "time-grunt": "^1.2.2",
    "url-loader": "^0.5.5",
    "webdrivercss": "^1.1.10",
    "webdriverio": "wbeard/webdriverio",
    "webpack": "^1.7.3",
    "webpack-dev-server": "^1.10.1",
    "xss": "^0.1.15"
  },
  "peerDependencies": {
    "react": "0.13.3"
  },
  "publishConfig": {
    "registry": "http://npm-publish.f4tech.com"
  },
  "devEngines": {
    "node": "0.10.41",
    "npm": "2.14.13"
  }
}
